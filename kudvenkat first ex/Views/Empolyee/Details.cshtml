@model kudvenkat_first_ex.Models.Empolyee

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<table style="font-family:Arial">
    <tr>
        <td> Employee ID</td>
        <td>@Model.EmployeeId </td>
    </tr>
    <tr>
        <td> Employee Name</td>
        <td>@Model.Name </td>
    </tr>
    <tr>
        <td> Employee Gender</td>
        <td>@Model.Gender </td>
    </tr>
    <tr>
        <td> Employee City</td>
        <td>@Model.City </td>
    </tr>
</table>

